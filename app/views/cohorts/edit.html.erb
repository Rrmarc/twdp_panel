<h1>Cohorts Edit</h1>


<%= form_for cohorts_path(@cohorts) do |f| %>
    
     <%= f.label 'Cohort Name:' %><br>
     <%= f.text_field :name %><br>
     
    <%= f.label 'Start Year' %><br>
    <%= f.text_area :start_date %><br>

    <%= f.label 'End Year' %><br>
    <%= f.text_area :end_date %><br>

    <%= f.submit "Submit Changes"%>
  <% end %>